import nemo;
import vm_translator;
import vmnemo;

main() {
    prog = "
var x : int;
var arr : [int];
{   
    x:=0;
    {
        {{
            arr := upd(arr, x, x);
            x := x + 1;
            (x<10)?;
        } U {
            x := 10;
            print(x);
            x := x - 1;
            (x > 0)?;
        }}
    }*;
    print(arr);
}
";
    nemo = s2ast(prog);
    vm = nemo2nvm(nemo);
    printvmProg(vm);
    exec(vm);
    quit(0);
}
